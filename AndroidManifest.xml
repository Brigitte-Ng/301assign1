<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travalexpensetracker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.travalexpensetracker.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.travalexpensetracker.ExpenseListActivity"
            android:label="@string/title_activity_expense_list" >
        </activity>
        <activity
            android:name="com.example.travalexpensetracker.ClaimListActivity"
            android:label="@string/title_activity_claim_list" >
        </activity>
        <activity
            android:name="com.example.travalexpensetracker.AddClaimActivity"
            android:label="@string/title_activity_add_claim" >
        </activity>
        <activity
            android:name="com.example.travalexpensetracker.AddExpenseActivity"
            android:label="@string/title_activity_add_expense" >
        </activity>
        <activity
            android:name="com.example.travalexpensetracker.Claim"
            android:label="@string/title_activity_view_claim" >
        </activity>
        <activity
            android:name="com.example.travalexpensetracker.ViewExpenseActivity"
            android:label="@string/title_activity_view_expense" >
        </activity>
        <activity
            android:name="com.example.travalexpensetracker.Expense"
            android:label="@string/title_activity_view_expense" >
        </activity>
    </application>

</manifest>
